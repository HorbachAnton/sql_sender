<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
    <class name="org.horbach.iba.sql_sender.entity.RequestResult" table="request_result" catalog="sql_sender">
        <id name="id" type="int">
            <column name="id" />
            <generator class="increment" />
        </id>
        <property name="message" type="java.lang.String">
            <column name="message" length="600" not-null="true"/>
        </property>
    </class>
</hibernate-mapping>
