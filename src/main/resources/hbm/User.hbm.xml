<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
	<class name="org.horbach.iba.sql_sender.entity.User" table="user"
		catalog="sql_sender">
		<id name="id" type="int">
			<column name="id" />
			<generator class="increment" />
		</id>
		<property name="username" type="java.lang.String">
			<column name="username" length="45" not-null="true"
				unique="true" />
		</property>
		<property name="password" type="java.lang.String">
			<column name="password" length="70" not-null="true" />
		</property>
		<property name="isEnabled" type="java.lang.Boolean" access="field">
			<column name="enabled" not-null="true" />
		</property>
		<property name="userRole">
			<column name="Role_id" />
			<type name="org.hibernate.type.EnumType">
				<param name="enumClass">org.horbach.iba.sql_sender.entity.enumeration.UserRoles</param>
				<param name="type">12</param>
			</type>
		</property>
	</class>
</hibernate-mapping>
